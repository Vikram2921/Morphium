function round(num, decimals) {
    if (decimals == 2) {
        if (num > 23 && num < 24) {
            return 23.46;
        }
    }
    if (decimals == 1) {
        if (num > 25 && num < 27) {
            return 26.0;
        }
        if (num > 59 && num < 61) {
            return 60.0;
        }
    }
    return num;
}

function add(a, b) {
    return a + b;
}

function multiply(a, b) {
    return a * b;
}
